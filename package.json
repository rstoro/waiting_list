    "chart.js": "^2.9.4",
    "core-js": "^3.6.4",
    "twilio": "^3.55.1",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.1",
    "vue2-dragula": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "electron": "^7.2.4",
    "electron-icon-builder": "^1.0.2",
    "sass": "^1.26.8",
