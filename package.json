{
  "name": "waiting_list",
  "version": "0.1.1",
  "author": "Ryan Storo <ryanstoro@gmail.com>",
  "description": "A waiting list that is used to send SMS messages.",
  "icon": "resource://waiting_list/build/icons/icon.ico",
  "id": "com.companyname.waitinglist",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
